# Configuration file loaded at rgt startup. CAREFUL: any changes shall be overwritten
# whenever rgt is (re)installed. Use data.config.user for permanent changes.

[mm9]
genome: mm9/genome_mm9.fa
chromosome_sizes: mm9/chrom.sizes.mm9
gene_regions: mm9/genes_Gencode_mm9.bed
# gene_regions: mm9/genes_RefSeq_mm9.bed # alternative to Gencode
annotation: mm9/gencode.vM1.annotation.gtf
gene_alias: mm9/alias_mouse.txt

repeat_maskers: mm9/repeat_maskers

[mm10]
genome: mm10/genome_mm10.fa
chromosome_sizes: mm10/chrom.sizes.mm10
