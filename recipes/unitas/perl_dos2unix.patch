+				if(lc$head[2].'_'.$head[3]ne$species&&!$miRhairpin_species{$hairpin_seq}) # species is different and sequence is not in species
+					{
+					$i++;
+					$miRhairpin_nonspecies{$hairpin_seq}.="$head[0]_stem-loop ";
+					}
+				@head=split(' ',$_);
+				$hairpin_seq="";
+				}
+			else
+				{
+				$_=~s/U/T/g;
+				$hairpin_seq.=$_;
+				}
+			}
+		if(lc$head[2].'_'.$head[3]ne$species&&!$miRhairpin_species{$hairpin_seq})
+			{
+			$i++;
+			$miRhairpin_nonspecies{$hairpin_seq}.="$head[0]_stem-loop ";
+			}
+		close MIR_HAIRPIN;
+		foreach(keys%miRhairpin_nonspecies)
+			{
+			$miRhairpin_nonspecies{$_}=~s/ $//;
+			}
+		if(!$silent){print" non-$species{$species}: $i.";}
+
+		#sort keys to make reproducable files
+		@keyshairpin_species=keys%miRhairpin_species;
+		@keyshairpin_species=sort@keyshairpin_species;
+		foreach$miR(@keyshairpin_species)
+			{
+			print MIR_HAIRPIN_SPECIES">$miRhairpin_species{$miR}\n$miR\n";
+			}
+		@keyshairpin_nonspecies=keys%miRhairpin_nonspecies;
+		@keyshairpin_nonspecies=sort@keyshairpin_nonspecies;
+		foreach$miR(@keyshairpin_nonspecies)
+			{
+			print MIR_HAIRPIN_NONSPECIES">$miRhairpin_nonspecies{$miR}\n$miR\n";
+			}
+		undef%miRhairpin_species;
+		undef%miRhairpin_nonspecies;
+		undef@keyshairpin_species;
+		undef@keyshairpin_nonspecies;
+		close MIR_HAIRPIN_SPECIES;
+		close MIR_HAIRPIN_NONSPECIES;
+		if(!$silent){print" OK.\n";}
+
+		open(DBVERSIONS,">UNITAS_refdump_$species/db_versions.info")||print"\n$!\n";
+		print DBVERSIONS$db_versions_info;
+		close DBVERSIONS;
+		}
+	}
