-$min_phasi_loci=5; # minimum number of phased loci within the sliding window
-$max_allowed_strandbias=0.95; # maximum strand bias of mapped reads. Avoid false positive phasiRNA prediction (e.g. rRNA fragments)
-$min_in_line=0; # minimum number of loci directly adjacent to each other
-$min_distance_between_clusters=1000; # minimum distance of loci to allocate different cluster ID
-$phasi_sw_size=1000; # must be an even number
-$phasi_p=0.05;
